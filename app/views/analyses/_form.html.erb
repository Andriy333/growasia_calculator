<%= form_for(analysis) do |f| %>
  <div id="wizard">
    <%= render partial: 'geo_location', f: f %>
    <%= render partial: 'basic_info', f: f %>
    <%= render partial: 'tillage', f: f %>
    <%= render partial: 'additional_crop_management', f: f %>
    <%= render partial: 'shade_trees', f: f %>
    <%= render partial: 'agrochemicals', f: f %>
    <%= render partial: 'fossil_fuels', f: f %>
    <h1>Done</h1>
    <section>
      <div class="actions">
        <%= f.submit %>
      </div>
    </section>
  </div>
<% end %>
